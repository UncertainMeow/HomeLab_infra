# Generated by Ansible for Technitium DNS Server
# Original resolv.conf backed up to /etc/resolv.conf.backup

# Use local Technitium DNS server first
nameserver 127.0.0.1

# Fallback to reliable public DNS servers
nameserver 1.1.1.1
nameserver 8.8.8.8

# Search domain
search {{ technitium_domain }}

options timeout:2 attempts:3 rotate single-request-reopen